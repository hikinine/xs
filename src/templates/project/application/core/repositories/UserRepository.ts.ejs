import { Prisma } from '@prisma/client';
import { User } from './../domain/entities/User';
import { Many } from "../domain/interface/contract";

export interface UserRepository {

  create(toCreate: User): Promise<User>;
  update(toUpdate: User): Promise<User>;
  delete(user_id: string): Promise<User>;


  findById(user_id: string, options?: Partial<Prisma.userFindUniqueArgs> ): Promise<User>
  findAll(options?: Partial<Prisma.userFindManyArgs> ): Promise<Many<User>>
}